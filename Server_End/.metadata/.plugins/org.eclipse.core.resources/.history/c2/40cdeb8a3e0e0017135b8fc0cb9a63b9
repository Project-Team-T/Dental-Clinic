package com.example.blog;

public class Blogidtitle_p {
	private int id;
	private String title;
	
	Blogidtitle_p(){}
	
	Blogidtitle_p(int id){
		this.id = id;
//		this.title = title;
	}
	
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public String getTitle() {
		return title;
	}
	public void setTitle(String title) {
		this.title = title;
	}
}
